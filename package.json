{
  "name": "hack-europe",
  "private": true,
  "scripts": {
    "dev": "concurrently -n ui,api -c cyan,magenta \"npm run dev:ui\" \"npm run dev:api\"",
    "dev:ui": "npm run dev --prefix ui",
    "dev:api": "cd api && python -m uvicorn index:app --reload --host 0.0.0.0",
    "api:gen": "(cd api && python export_openapi.py) && npm run api:gen --prefix ui",
    "migrate": "tsx scripts/migrate.ts",
    "docker:build": "docker build -f api/Dockerfile -t hack-europe-api api",
    "docker:run": "docker run --rm -p 8000:8000 --env-file .env hack-europe-api",
    "deploy:ecr": "docker build --platform linux/amd64 -f api/Dockerfile -t 296410404869.dkr.ecr.us-west-2.amazonaws.com/hack-europe:latest api"
  },
  "devDependencies": {
    "@types/pg": "^8.11.6",
    "concurrently": "^9.1.2",
    "dotenv": "^16.4.5",
    "pg": "^8.13.0",
    "tsx": "^4.19.2"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/material": "^7.3.8",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "motion": "^12.34.3",
    "next-themes": "^0.4.6",
    "react-day-picker": "^9.13.2",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-hook-form": "^7.71.2",
    "react-popper": "^2.3.0",
    "react-resizable-panels": "^4.6.5",
    "react-responsive-masonry": "^2.7.1",
    "react-router": "^7.13.0",
    "react-slick": "^0.31.0",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "vaul": "^1.1.2"
  }
}
